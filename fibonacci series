import java.util.*;
public class Fibonacci
{
    public static void main(String[] args)
    {
        int i,n,a=0,b=1,c=1,d=0;
        Scanner in = new Scanner(System.in);
        System.out.println(a+" "+b+" "+c);
        System.out.println("Enter the term upto which you want to print fibonacci series");
        n=in.nextInt();
        for(i=1;i<=n;i++)
        {
            d=b+c;
            System.out.print(d+" ");
            b=c;
            c=d;
        }
        
    }
}
